{
  "name": "phononet-users",
  "version": "0.7.2",
  "author": "Siegfried Wanke <swanke@phononet.de>",
  "summary": "This module manage the linux user",
  "license": "Apache-2.0",
  "source": "http://repo.phononet.local/hg/admin/puppet-modules/phononet-users",
  "project_page": "http://trac.phononet.local/admin",
